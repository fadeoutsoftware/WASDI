		<dependency>
			<groupId>org.assertj</groupId>
			<artifactId>assertj-core</artifactId>
			<!-- use version 2.1.0 or higher -->
			<version>2.1.0</version>
			<scope>test</scope>
		</dependency>
		<!-- KML -->

	</dependencies>

	<repositories>
		<repository>
			<id>GeoSolutions</id>
			<url>http://maven.geo-solutions.it/</url>
		</repository>
